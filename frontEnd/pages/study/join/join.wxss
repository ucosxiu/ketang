/* pages/study/join/join.wxss */
.weui-cell{
	transition:all .6s ease-in-out 0s;
	transform:translateX(0rpx);
}
.move-cur {
	transform:translateX(-200rpx);
}
.weui-cell .move {
	position:absolute;
	right:0;
	display:flex;
	width:200rpx;
	height:100%;
	transform:translateX(100%);
}
.move view {
	display:flex;
	flex:1;
	justify-content:center;
	align-items:center;
}

.btn-del{
	background: #ff0000;
	color: #ffffff;
}
